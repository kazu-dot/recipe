<div>タグ一覧
    <% @tag_list.each do |tag| %>
      <div class="tag-content">
        <%= link_to tag_microposts_path(tag_id: tag.id), class:"micropost-select" do %>
          <span style="font-size: 18px;">
            <%= tag.tag_name %>
          </span>
        <% end %>
      </div>
    <% end %>
  </div>
